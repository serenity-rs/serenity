{
    "problemMatcher": [
        {
            "owner": "cargo-common",
            "pattern": [
                {
                    "regexp": "^(warning|warn|error)(\\[(\\S*)\\])?: (.*)$",
                    "severity": 1,
                    "message": 4,
                    "code": 3
                },
                {
                    "regexp": "^\\s+-->\\s(\\S+):(\\d+):(\\d+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3
                }
            ]
        },
        {
            "owner": "cargo-test",
            "pattern": [
                {
                    "regexp": "^.*panicked\\s+at\\s+'(.*)',\\s+(.*):(\\d+):(\\d+)$",
                    "message": 1,
                    "file": 2,
                    "line": 3,
                    "column": 4
                }
            ]
        },
        {
            "owner": "cargo-fmt",
            "pattern": [
                {
                    "regexp": "^(Diff in (\\S+)) at line (\\d+):",
                    "message": 1,
                    "file": 2,
                    "line": 3
                }
            ]
        }
    ]
}
